#import "ColorManagement.h"

@implementation ColorManagement



#define aaa = [UIColor colorWithRed:230/255.0f green:230/255.0f blue:230/255.0f alpha:1];


+ (NSDictionary*)getColorMappings {
    static NSDictionary* dictionary;
    
    if (dictionary == nil)
        dictionary = [[NSDictionary alloc] initWithObjectsAndKeys:
               [UIColor colorWithRed:230/255.0f green:230/255.0f blue:230/255.0f alpha:1],      @"TableCellSelectedGrey",
               [UIColor colorWithRed:59/255.0f green:59/255.0f blue:59/255.0f alpha:1],         @"BackgroundGrey",
               [UIColor cyanColor],                                                             @"ButtonBlue",
               [UIColor orangeColor],                                                           @"ButtonOrange",
            nil];
    
    return dictionary;
}





@end
