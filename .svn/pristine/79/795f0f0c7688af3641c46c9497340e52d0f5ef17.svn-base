#import "MMRestNotImplementedException.h"

NSString *MMRestNotImplementedExceptionName = @"MMRestNotImplementedException";

@implementation MMRestNotImplementedException

- (id) initWithReason:(NSString *)reason userInfo:(NSDictionary *)userInfo {
    self = [super initWithName:MMRestNotImplementedExceptionName reason:reason userInfo:userInfo nestedException:nil containedError:nil];
    if (self) {
        return self;
    }
    
    return nil;
}

@end
